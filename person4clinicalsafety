[gaddam.h@explorer-01 person4]$ cat > analyze_guides.sh << 'ENDOFSCRIPT'
#!/bin/bash

echo "Analyzing CCR5 guides..."
mkdir -p reports

# Extract unique guide sequences
awk -F'\t' 'NR>1 {print $2}' ccr5_analysis/ccr5_reference_annotated.txt.Annotation.targets.txt | sort -u > reports/all_guides.txt

# Create CSV header
echo "Guide_Sequence,Total_OffTargets_Ref,Total_OffTargets_Enr,Gene_Hits,Intergenic" > reports/guide_summary.csv

# Process each guide
while read guide; do
  ref_count=$(grep -F "$guide" ccr5_analysis/ccr5_reference_annotated.txt.Annotation.targets.txt | wc -l)
  enr_count=$(grep -F "$guide" ccr5_analysis/ccr5_enriched_annotated.txt.Annotation.targets.txt | wc -l)
  gene_hits=$(grep -F "$guide" ccr5_analysis/ccr5_reference_annotated.txt.Annotation.targets.txt | awk -F'\t' '{print $NF}' | grep -v "^n$" | wc -l)
  intergenic=$(grep -F "$guide" ccr5_analysis/ccr5_reference_annotated.txt.Annotation.targets.txt | awk -F'\t' '{print $NF}' | grep "^n$" | wc -l)
  echo "$guide,$ref_count,$enr_count,$gene_hits,$intergenic"
done < reports/all_guides.txt >> reports/guide_summary.csv

echo "Done! Check reports/guide_summary.csv"
./analyze_guides.shides.sh
Analyzing CCR5 guides...
Done! Check reports/guide_summary.csv
