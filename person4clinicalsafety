#!/bin/bash

echo "Analyzing CCR5 guides..."

# Create output directory
mkdir -p reports

# Extract guide sequences
awk -F'\t' 'NR>1 {print $2}' ccr5_analysis/ccr5_reference_annotated.txt.Annotation.targets.txt | \
  sort -u > reports/all_guides.txt

