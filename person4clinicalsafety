#!/bin/bash
#SBATCH --job-name=ccr5_search
#SBATCH --output=logs/ccr5_search_%j.out
#SBATCH --error=logs/ccr5_search_%j.err
#SBATCH --time=02:00:00
#SBATCH --ntasks=1
#SBATCH --cpus-per-task=4
#SBATCH --mem=16G
#SBATCH --partition=short

cd ~/crispritz_repro

echo "Starting CCR5 reference search at $(date)"

apptainer exec crispritz.sif crispritz.py search \
  genome_library/NGG_2_hg19_reference \
  pam/pamNGG.txt \
  guides/ccr5_guides.txt \
  results/person4/ccr5_analysis/ccr5_reference_FIXED \
  -mm 4 \
  -bDNA 0 \
  -bRNA 0 \
  -th 4 \
EOFo "Job completed at $(date)"ysis/ccr5_*_FIXED.targets.txt
