  results/person4/ccr5_analysis/ccr5_enriched_FIXED.targets.txt \
  data/refGene_hg19.bed \
  results/person4/ccr5_analysis/ccr5_enriched_FIXED_annotated.txt

echo "Completed at $(date)"
